{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://eqds.org/schemas/equine-observation.schema.json",
  "title": "Equine Observation Profile",
  "description": "JSON Schema for Equine Observation Profile",
  "type": "object",
  "properties": {
    "resourceType": {
      "type": "string",
      "const": "Observation"
    },
    "id": {
      "type": "string",
      "pattern": "^[A-Za-z0-9\\-\\.]{1,64}$"
    },
    "meta": {
      "type": "object",
      "properties": {
        "profile": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "contains": {
            "const": "https://eqds.org/StructureDefinition/equine-observation"
          }
        }
      }
    }
  },
  "required": [
    "resourceType"
  ],
  "definitions": {}
}